<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_35"
        android:paddingTop="@dimen/dp_15"
        android:background="@drawable/shape_main_bg"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="填写个人信息"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:textStyle="bold" />

        <com.stkj.infocollect.card.ui.widget.CardAccountInfoLayout
            android:id="@+id/account_info_lay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_30" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_32"
            android:gravity="center">

            <com.stkj.common.ui.widget.shapelayout.ShapeTextView
                android:id="@+id/stv_cancel"
                android:layout_width="@dimen/dp_75"
                android:layout_height="@dimen/dp_23"
                android:gravity="center"
                android:text="取消"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/sp_11"
                app:radiusT="@dimen/dp_4"
                app:solidColorT="@color/white"
                app:strokeColorT="@color/color_D7D7D7"
                app:strokeWidthT="@dimen/dp_0_5" />

            <com.stkj.common.ui.widget.shapelayout.ShapeTextView
                android:id="@+id/stv_upload"
                android:layout_width="@dimen/dp_75"
                android:layout_height="@dimen/dp_23"
                android:layout_marginLeft="@dimen/dp_20"
                android:gravity="center"
                android:text="提交"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_11"
                app:radiusT="@dimen/dp_4"
                app:solidColorT="@color/color_1890FF"/>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>